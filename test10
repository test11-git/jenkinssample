public class Test10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int num=5;
	
		  System.out.println(Integer.MAX_VALUE);
		  long factorial=1;
		 // int factorial=1;
		  for(int i=1;i<=num;i++)
		   { factorial=factorial*i;
		   }
		 System.out.println("factorial is:"+factorial);
		
	}

}
